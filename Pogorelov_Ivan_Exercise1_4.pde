size(200,200);//create the window
strokeWeight(3);//set up the thikeness of the dividing lines
fill(0,0,0);//color of the black square (top left)
rectMode(CORNER);//mode of rectange
rect(0,0,100,100);//top left square
fill(255,255,255);//color of the white square (top right)
rect(100,0,100,100);//top right square
fill(250,250,250);//color of the light gray square (bottom left)
rect(0,100,100,100);//bottom left square
fill(100,100,100);//color of the dark gray square (bottom right)
rect(100,100,100,100);//bottom right square
